@article{staudinger_versatile_2018,
	title = {A versatile optode system for oxygen, carbon dioxide, and {pH} measurements in seawater with integrated battery and logger: \textit{ {A} versatile optode system for {O} $_{\textrm{2}}$ , {CO} $_{\textrm{2}}$ , and {pH} }},
	volume = {16},
	copyright = {All rights reserved},
	issn = {15415856},
	shorttitle = {A versatile optode system for oxygen, carbon dioxide, and {pH} measurements in seawater with integrated battery and logger},
	url = {http://doi.wiley.com/10.1002/lom3.10260},
	doi = {10.1002/lom3.10260},
	abstract = {Herein, we present a small and versatile optode system with integrated battery and logger for monitoring of O2, pH, and pCO2 in seawater. Three sensing materials designed for seawater measurements are optimized with respect to dynamic measurement range and long-term stability. The spectral properties of the sensing materials were tailored to be compatible with a commercially available laboratory oxygen logger that was ﬁtted into a pressure housing. Interchangeable sensor caps with appropriate “sensing chemistry” are conveniently attached to the end of the optical ﬁber. This approach allows using the same instrument for multiple analytes, which offers great ﬂexibility and minimizes hardware costs. Applications of the new optode system were demonstrated by recording depth proﬁles for the three parameters during a research cruise in the Baltic Sea and by measuring surface water transects of pH. The optode was furthermore used to monitor the concentration of dissolved oxygen in a seagrass meadow in the Limfjord, Denmark, and sensor packages consisting of pO2, pH, and pCO2 were deployed in the harbors of Kiel, Germany, and Southampton, England, for 6 d. The measurements revealed that the system can resolve typical patterns in seawater chemistry related to spatial heterogeneities as well as temporal changes caused by biological and tidal activity.},
	language = {en},
	number = {7},
	urldate = {2019-10-10},
	journal = {Limnology and Oceanography: Methods},
	author = {Staudinger, Christoph and Strobl, Martin and Fischer, Jan P. and Thar, Roland and Mayr, Torsten and Aigner, Daniel and Müller, Bernhard J. and Müller, Bernhard and Lehner, Philipp and Mistlberger, Günter and Fritzsche, Eva and Ehgartner, Josef and Zach, Peter W. and Clarke, Jennifer S. and Geißler, Felix and Mutzberg, André and Müller, Jens D. and Achterberg, Eric P. and Borisov, Sergey M. and Klimant, Ingo},
	month = jul,
	year = {2018},
	pages = {459--473},
	file = {Staudinger et al. - 2018 - A versatile optode system for oxygen, carbon dioxi.pdf:/home/jens/Zotero/storage/7LZPETEP/Staudinger et al. - 2018 - A versatile optode system for oxygen, carbon dioxi.pdf:application/pdf},
}

@article{honkanen_diurnal_2021,
	title = {The diurnal cycle of \textit{p}{CO}$_{\textrm{2}}$ in the coastal region of the {Baltic} {Sea}},
	volume = {17},
	copyright = {Creative Commons Attribution-ShareAlike 4.0 International Licence (CC-BY-SA)},
	issn = {1812-0784},
	url = {https://os.copernicus.org/articles/17/1657/2021/},
	doi = {10.5194/os-17-1657-2021},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-contentHeaderColor"{\textgreater}Abstract.{\textless}/strong{\textgreater} The direction and magnitude of carbon dioxide fluxes between the atmosphere and the sea are regulated by the gradient in the partial pressure of carbon dioxide ({\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater}) across the air–sea interface. Typically, observations of {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} at the sea surface are carried out by using research vessels and ships of opportunity, which usually do not resolve the diurnal cycle of {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} at a given location. This study evaluates the magnitude and driving processes of the diurnal cycle of {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} in a coastal region of the Baltic Sea. We present {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} data from July 2018 to June 2019 measured in the vicinity of the island of Utö at the outer edge of the Archipelago Sea, and quantify the relevant physical, biological, and chemical processes controlling {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater}. The highest monthly median of diurnal {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} variability (31 {\textless}span class="inline-formula"{\textgreater}µatm{\textless}/span{\textgreater}) was observed in August and predominantly driven by biological processes. Biological fixation and mineralization of carbon led to sinusoidal diurnal {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} variations, with a maximum in the morning and a minimum in the afternoon. Compared with the biological carbon transformations, the impacts of air–sea fluxes and temperature changes on {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} were small, with their contributions to the monthly medians of diurnal {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} variability being up to 12 and 5 {\textless}span class="inline-formula"{\textgreater}µ{\textless}/span{\textgreater}atm, respectively. During upwelling events, short-term {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} variability (up to 500 {\textless}span class="inline-formula"{\textgreater}µ{\textless}/span{\textgreater}atm within a day) largely exceeded the usual diurnal cycle. If the net annual air–sea flux of carbon dioxide at our study site and for the sampled period is calculated based on a data subset that consists of only one regular measurement per day, the bias in the net exchange depends on the sampling time and can amount up to {\textless}span class="inline-formula"{\textgreater}±12{\textless}/span{\textgreater} \%. This finding highlights the importance of continuous surface {\textless}span class="inline-formula"{\textgreater}\textit{p}CO$_{\textrm{2}}${\textless}/span{\textgreater} measurements at fixed locations for the assessment of the short-term variability of the carbonate system and the correct determination of air–sea {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} fluxes.{\textless}/p{\textgreater}},
	language = {English},
	number = {6},
	urldate = {2022-01-13},
	journal = {Ocean Science},
	author = {Honkanen, Martti and Müller, Jens Daniel and Seppälä, Jukka and Rehder, Gregor and Kielosto, Sami and Ylöstalo, Pasi and Mäkelä, Timo and Hatakka, Juha and Laakso, Lauri},
	month = nov,
	year = {2021},
	note = {Publisher: Copernicus GmbH},
	pages = {1657--1675},
	file = {Honkanen et al. - 2021 - The diurnal cycle of pCO2 in the.pdf:/home/jens/Zotero/storage/WRAQ2X95/Honkanen et al. - 2021 - The diurnal cycle of pCO2 in the.pdf:application/pdf},
}

@article{jacobs_upwelling-induced_2021,
	title = {Upwelling-induced trace gas dynamics in the {Baltic} {Sea} inferred from 8 years of autonomous measurements on a ship of opportunity},
	volume = {18},
	copyright = {All rights reserved},
	issn = {1726-4170},
	url = {https://bg.copernicus.org/articles/18/2679/2021/},
	doi = {10.5194/bg-18-2679-2021},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-contentHeaderColor"{\textgreater}Abstract.{\textless}/strong{\textgreater} Autonomous measurements aboard ships of opportunity (SOOP) provide in situ data sets with high spatial and temporal coverage. In this study, we use 8 years of carbon dioxide ({\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater}) and methane ({\textless}span class="inline-formula"{\textgreater}CH$_{\textrm{4}}${\textless}/span{\textgreater}) observations from SOOP \textit{Finnmaid} to study the influence of upwelling on trace gas dynamics in the Baltic Sea. Between spring and autumn, coastal upwelling transports water masses enriched with {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} and {\textless}span class="inline-formula"{\textgreater}CH$_{\textrm{4}}${\textless}/span{\textgreater} to the surface of the Baltic Sea. We study the seasonality, regional distribution, relaxation, and interannual variability in this process. We use reanalysed wind and modelled sea surface temperature (SST) data in a newly established statistical upwelling detection method to identify major upwelling areas and time periods. Large upwelling-induced SST decrease and trace gas concentration increase are most frequently detected around August after a long period of thermal stratification, i.e. limited exchange between surface and underlying waters. We found that these upwelling events with large SST excursions shape local trace gas dynamics and often lead to near-linear relationships between increasing trace gas levels and decreasing temperature. Upwelling relaxation is mainly driven by mixing, modulated by air–sea gas exchange, and possibly primary production. Subsequent warming through air–sea heat exchange has the potential to enhance trace gas saturation. In 2015, quasi-continuous upwelling over several months led to weak summer stratification, which directly impacted the observed trace gas and SST dynamics in several upwelling-prone areas. Trend analysis is still prevented by the observed high variability, uncertainties from data coverage, and long water residence times of 10–30 years. We introduce an extrapolation method based on trace gas–SST relationships that allows us to estimate upwelling-induced trace gas fluxes in upwelling-affected regions. In general, the surface water reverses from {\textless}span class="inline-formula"{\textgreater}CO$_{\textrm{2}}${\textless}/span{\textgreater} sink to source, and {\textless}span class="inline-formula"{\textgreater}CH$_{\textrm{4}}${\textless}/span{\textgreater} outgassing is intensified as a consequence of upwelling. We conclude that SOOP data, especially when combined with other data sets, enable flux quantification and process studies addressing the process of upwelling on large spatial and temporal scales.{\textless}/p{\textgreater}},
	language = {English},
	number = {8},
	urldate = {2022-01-13},
	journal = {Biogeosciences},
	author = {Jacobs, Erik and Bittig, Henry C. and Gräwe, Ulf and Graves, Carolyn A. and Glockzin, Michael and Müller, Jens D. and Schneider, Bernd and Rehder, Gregor},
	month = apr,
	year = {2021},
	note = {Publisher: Copernicus GmbH},
	pages = {2679--2709},
	file = {Jacobs et al. - 2021 - Upwelling-induced trace gas dynamics in the Baltic.pdf:/home/jens/Zotero/storage/WHHDW6S8/Jacobs et al. - 2021 - Upwelling-induced trace gas dynamics in the Baltic.pdf:application/pdf},
}

@article{sanders_decoupling_2021,
	title = {Decoupling salinity and carbonate chemistry: low calcium ion concentration rather than salinity limits calcification in {Baltic} {Sea} mussels},
	volume = {18},
	copyright = {All rights reserved},
	issn = {1726-4170},
	shorttitle = {Decoupling salinity and carbonate chemistry},
	url = {https://bg.copernicus.org/articles/18/2573/2021/},
	doi = {10.5194/bg-18-2573-2021},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-contentHeaderColor"{\textgreater}Abstract.{\textless}/strong{\textgreater} The Baltic Sea has a salinity gradient decreasing from fully marine ({\textless}span class="inline-formula"{\textgreater}\&gt;{\textless}/span{\textgreater} 25) in the west to below 7 in the central Baltic Proper. Habitat-forming and ecologically dominant mytilid mussels exhibit decreasing growth when salinity {\textless}span class="inline-formula"{\textgreater}\&lt;{\textless}/span{\textgreater} 11; however, the mechanisms underlying reduced calcification rates in dilute seawater are not fully understood. Both [HCO{\textless}span class="inline-formula"{\textgreater}{\textless}math xmlns="http://www.w3.org/1998/Math/MathML" id="M3" display="inline" overflow="scroll" dspmath="mathml"{\textgreater}{\textless}mrow{\textgreater}{\textless}msubsup{\textgreater}{\textless}mi/{\textgreater}{\textless}mn mathvariant="normal"{\textgreater}3{\textless}/mn{\textgreater}{\textless}mo{\textgreater}-{\textless}/mo{\textgreater}{\textless}/msubsup{\textgreater}{\textless}/mrow{\textgreater}{\textless}/math{\textgreater}{\textless}span{\textgreater}{\textless}svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="9pt" height="16pt" class="svg-formula" dspmath="mathimg" md5hash="0aaab3ee324d7a9ba8e4b96f67d8036e"{\textgreater}{\textless}svg:image xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="bg-18-2573-2021-ie00001.svg" width="9pt" height="16pt" src="bg-18-2573-2021-ie00001.png"/{\textgreater}{\textless}/svg:svg{\textgreater}{\textless}/span{\textgreater}{\textless}/span{\textgreater}] and [Ca{\textless}span class="inline-formula"{\textgreater}$^{\textrm{2+}}${\textless}/span{\textgreater}] also decrease with salinity, challenging calcifying organisms through CaCO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{3}}${\textless}/span{\textgreater} undersaturation ({\textless}span class="inline-formula"{\textgreater}Ω≤1{\textless}/span{\textgreater}) and unfavourable ratios of calcification substrates ([Ca{\textless}span class="inline-formula"{\textgreater}$^{\textrm{2+}}${\textless}/span{\textgreater}] and [HCO{\textless}span class="inline-formula"{\textgreater}{\textless}math xmlns="http://www.w3.org/1998/Math/MathML" id="M8" display="inline" overflow="scroll" dspmath="mathml"{\textgreater}{\textless}mrow{\textgreater}{\textless}msubsup{\textgreater}{\textless}mi/{\textgreater}{\textless}mn mathvariant="normal"{\textgreater}3{\textless}/mn{\textgreater}{\textless}mo{\textgreater}-{\textless}/mo{\textgreater}{\textless}/msubsup{\textgreater}{\textless}/mrow{\textgreater}{\textless}/math{\textgreater}{\textless}span{\textgreater}{\textless}svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="9pt" height="16pt" class="svg-formula" dspmath="mathimg" md5hash="7de8959157e6c258409d4c11688ca166"{\textgreater}{\textless}svg:image xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="bg-18-2573-2021-ie00002.svg" width="9pt" height="16pt" src="bg-18-2573-2021-ie00002.png"/{\textgreater}{\textless}/svg:svg{\textgreater}{\textless}/span{\textgreater}{\textless}/span{\textgreater}]) to the inhibitor (H{\textless}span class="inline-formula"{\textgreater}$^{\textrm{+}}${\textless}/span{\textgreater}), expressed as the extended substrate–inhibitor ratio (ESIR). This study combined in situ monitoring of three southwest Baltic mussel reefs with two laboratory experiments to assess how various environmental conditions and isolated abiotic factors (salinity, [Ca{\textless}span class="inline-formula"{\textgreater}$^{\textrm{2+}}${\textless}/span{\textgreater}], [HCO{\textless}span class="inline-formula"{\textgreater}{\textless}math xmlns="http://www.w3.org/1998/Math/MathML" id="M11" display="inline" overflow="scroll" dspmath="mathml"{\textgreater}{\textless}mrow{\textgreater}{\textless}msubsup{\textgreater}{\textless}mi/{\textgreater}{\textless}mn mathvariant="normal"{\textgreater}3{\textless}/mn{\textgreater}{\textless}mo{\textgreater}-{\textless}/mo{\textgreater}{\textless}/msubsup{\textgreater}{\textless}/mrow{\textgreater}{\textless}/math{\textgreater}{\textless}span{\textgreater}{\textless}svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="9pt" height="16pt" class="svg-formula" dspmath="mathimg" md5hash="fa1148a5a7ab62133104fb46bf612014"{\textgreater}{\textless}svg:image xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="bg-18-2573-2021-ie00003.svg" width="9pt" height="16pt" src="bg-18-2573-2021-ie00003.png"/{\textgreater}{\textless}/svg:svg{\textgreater}{\textless}/span{\textgreater}{\textless}/span{\textgreater}] and pH) impact calcification in mytilid mussels along the Baltic salinity gradient. Laboratory experiments rearing juvenile Baltic \textit{Mytilus} at a range of salinities (6, 11 and 16), HCO{\textless}span class="inline-formula"{\textgreater}{\textless}math xmlns="http://www.w3.org/1998/Math/MathML" id="M12" display="inline" overflow="scroll" dspmath="mathml"{\textgreater}{\textless}mrow{\textgreater}{\textless}msubsup{\textgreater}{\textless}mi/{\textgreater}{\textless}mn mathvariant="normal"{\textgreater}3{\textless}/mn{\textgreater}{\textless}mo{\textgreater}-{\textless}/mo{\textgreater}{\textless}/msubsup{\textgreater}{\textless}/mrow{\textgreater}{\textless}/math{\textgreater}{\textless}span{\textgreater}{\textless}svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="9pt" height="16pt" class="svg-formula" dspmath="mathimg" md5hash="ee54bb0fff66afdafaf51bed1fde360d"{\textgreater}{\textless}svg:image xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="bg-18-2573-2021-ie00004.svg" width="9pt" height="16pt" src="bg-18-2573-2021-ie00004.png"/{\textgreater}{\textless}/svg:svg{\textgreater}{\textless}/span{\textgreater}{\textless}/span{\textgreater} concentrations (300–2100 {\textless}span class="inline-formula"{\textgreater}µ{\textless}/span{\textgreater}mol kg{\textless}span class="inline-formula"{\textgreater}$^{\textrm{−1}}${\textless}/span{\textgreater}) and Ca{\textless}span class="inline-formula"{\textgreater}$^{\textrm{2+}}${\textless}/span{\textgreater} concentrations (0.5–4 mmol kg{\textless}span class="inline-formula"{\textgreater}$^{\textrm{−1}}${\textless}/span{\textgreater}) reveal that as individual factors, low [HCO{\textless}span class="inline-formula"{\textgreater}{\textless}math xmlns="http://www.w3.org/1998/Math/MathML" id="M17" display="inline" overflow="scroll" dspmath="mathml"{\textgreater}{\textless}mrow{\textgreater}{\textless}msubsup{\textgreater}{\textless}mi/{\textgreater}{\textless}mn mathvariant="normal"{\textgreater}3{\textless}/mn{\textgreater}{\textless}mo{\textgreater}-{\textless}/mo{\textgreater}{\textless}/msubsup{\textgreater}{\textless}/mrow{\textgreater}{\textless}/math{\textgreater}{\textless}span{\textgreater}{\textless}svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="9pt" height="16pt" class="svg-formula" dspmath="mathimg" md5hash="5a2143864edd3f7cf8f1639018917994"{\textgreater}{\textless}svg:image xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="bg-18-2573-2021-ie00005.svg" width="9pt" height="16pt" src="bg-18-2573-2021-ie00005.png"/{\textgreater}{\textless}/svg:svg{\textgreater}{\textless}/span{\textgreater}{\textless}/span{\textgreater}], pH and salinity cannot explain low calcification rates in the Baltic Sea. Calcification rates are impeded when {\textless}span class="inline-formula"{\textgreater}Ω$_{\textrm{aragonite}}${\textless}/span{\textgreater} {\textless}span class="inline-formula"{\textgreater}≤{\textless}/span{\textgreater} 1 or ESIR {\textless}span class="inline-formula"{\textgreater}≤{\textless}/span{\textgreater} 0.7 primarily due to [Ca{\textless}span class="inline-formula"{\textgreater}$^{\textrm{2+}}${\textless}/span{\textgreater}] limitation which becomes relevant at a salinity of ca. 11 in the Baltic Sea. Field monitoring of carbonate chemistry and calcification rates suggest increased food availability may be able to mask the negative impacts of periodic sub-optimal carbonate chemistry, but not when seawater conditions are permanently adverse, as observed in two Baltic reefs at salinities {\textless}span class="inline-formula"{\textgreater}\&lt;{\textless}/span{\textgreater} 11. Regional climate models predict a rapid desalination of the southwest and central Baltic over the next century and potentially a reduction in [Ca{\textless}span class="inline-formula"{\textgreater}$^{\textrm{2+}}${\textless}/span{\textgreater}] which may shift the distribution of marine calcifiers westward. It is therefore vital to understand the mechanisms by which the ionic composition of seawater impacts bivalve calcification for better predicting the future of benthic Baltic ecosystems.{\textless}/p{\textgreater}},
	language = {English},
	number = {8},
	urldate = {2022-01-13},
	journal = {Biogeosciences},
	author = {Sanders, Trystan and Thomsen, Jörn and Müller, Jens Daniel and Rehder, Gregor and Melzner, Frank},
